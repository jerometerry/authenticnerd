{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@scripts/*": [
        "./src/scripts/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ]
    }
  }
}